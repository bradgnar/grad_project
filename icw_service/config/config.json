{
    "express": {
        "mountpath": "/icw-service"
    },

    "databaseConfig": {
        "connectionString": "mongodb://localhost/icw"
    },

    "middleware": {

        "static": {
            "module": {
                "arguments": [ "path:./.build" ]
            }
        }

        ,"router": {
            "module": {
                "arguments": [{ "directory": "path:./controllers" }]
            }
        }

        
        ,"expressView": {
            "priority": 100,
            "enabled": true,
            "module": {
                "name": "engine-munger/lib/expressView"
            }
        }
        

        
    }
}

